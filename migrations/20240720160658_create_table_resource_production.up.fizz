create_table("resource_production") {
    t.Column("id", "integer", {primary: true})
    t.Column("country", "string")
    t.Column("code", "string")
    t.Column("year", "string")
    t.Column("gas_production", "string")
    t.Column("coal_production", "string")
    t.Column("oil_production", "string")
}